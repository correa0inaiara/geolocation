{
  "name": "geolocation-project",
  "version": "0.0.1",
  "description": "Project that explores some functionalities of a Geolocation API (Geoapify) and MongoDB's GeoJSON capabilities.",
  "main": "src/server.ts",
  "scripts": {
    "start": "node -r ts-node/register --env-file=.env src/server.ts",
    "ts-node": "ts-node src/server.ts",
    "dev": "nodemon --exec node --trace-warnings -r ts-node/register --env-file=.env src/server.ts",
    "test": "mocha --require ts-node/register ./src/**/*.spec.ts --exit",
    "build-app": "npm run build-client && npm install && npm run build-start",
    "build-client": "cd client && npm run build-app && cd ..",
    "build-start": "pm2 start src/server.ts -n Convertible9000 && pm2 monit",
    "babel": "babel --out-file bundle.js src/server.ts"
  },
  "author": "Naiara Correa",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.1.0",
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "@typegoose/typegoose": "^11.5.0",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "body-parser": "^1.20.3",
    "bunyan": "^1.8.15",
    "chai": "^4.3.10",
    "connect-history-api-fallback": "^2.0.0",
    "express": "^4.18.2",
    "express-handlebars": "^8.0.1",
    "handlebars": "^4.7.8",
    "i18next": "^24.2.2",
    "i18next-http-backend": "^3.0.2",
    "i18next-http-middleware": "^3.7.1",
    "mongoose": "^7.5.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "vue": "^3.5.13",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@eslint/js": "^9.20.0",
    "@types/chai": "^4.3.6",
    "@types/express": "^4.17.18",
    "@types/jquery": "^3.5.32",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sinon": "^10.0.17",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.15.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.5.1",
    "sinon": "^16.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.24.0"
  }
}
